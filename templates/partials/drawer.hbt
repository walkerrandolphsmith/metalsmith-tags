<section class="drawer">
    <i class="fa fa-times close"></i>

    <section class="widget blog-info">
            <a href="{{baseUrl}}">
                <img src="{{baseUrl}}/css/images/kakashi.png" class="retina" alt="Walker Smith">
            </a>
        <hr>
        <p>
            This is a tidbit about me!
            This is a tidbit about me!
            This is a tidbit about me!
            This is a tidbit about me!
        </p>
    </section>

    <section class="widget navigation">
        <!--
        {{>navigation }}
        -->
    </section>
    <section class="widget subscribe">
        <div class="inner">
            <h3 class="title">My Newsletter</h3>
            <hr>

            <p>
                Subscribe to my newsletter for a weekly look at the latest news, tools, and techniques from the React community.
            </p>
            <i class="fa fa-envelope-o"></i>

            <!-- Begin MailChimp Form -->
            <div id="mc_embed_signup">
                <form action="" method="post" id="mc-embedded-subscribe-form" name="mc-embedded-subscribe-form" class="validate" target="_blank" novalidate="">
                    <div id="mc_embed_signup_scroll">
                        <div class="mc-field-group">
                            <label for="mce-EMAIL">Email Address </label>
                            <input type="email" value="" name="EMAIL" class="required email" id="mce-EMAIL" placeholder="Email Address...">
                        </div>
                        <div id="mce-responses" class="clear">
                            <div class="response" id="mce-error-response" style="display:none"></div>
                            <div class="response" id="mce-success-response" style="display:none"></div>
                        </div>    <!-- real people should not fill this in and expect good things - do not remove this or risk form bot signups-->
                        <div style="position: absolute; left: -5000px;" aria-hidden="true"><input type="text" name="b_3c40623ce03f70afd7e8b6687_d572450014" tabindex="-1" value=""></div>
                        <div class="clear"><input type="submit" value="Subscribe" name="subscribe" id="mc-embedded-subscribe" class="button"></div>
                    </div>
                </form>
            </div>
            <!-- End MailChimp Form -->

        </div>
    </section>
    <section class="widget tags">
        <h3 class="title">Tags</h3>
        <hr>
        <div class="tagcloud">
            {{tagList this}}
        </div>
    </section>

    <section class="widget twitter">
        <h3 class="title">Twitter</h3>
        <hr>
        <a class="twitter-timeline"  href="https://twitter.com/WalkerRSmith" data-widget-id="398194465573052416">Tweets by @WalkerRSmith</a>
        <script>!function(d,s,id){var js,fjs=d.getElementsByTagName(s)[0],p=/^http:/.test(d.location)?'http':'https';if(!d.getElementById(id)){js=d.createElement(s);js.id=id;js.src=p+"://platform.twitter.com/widgets.js";fjs.parentNode.insertBefore(js,fjs);}}(document,"script","twitter-wjs");</script>
    </section>
    <section class="widget copyright">
        <p class="main">
            <span>Made with </span><i class="fa fa-heart"></i><span> by </span><a href="{{baseUrl}}">Walker Smith</a>
        </p>
    </section>

</section>