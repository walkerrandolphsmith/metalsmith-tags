  {{> header}}
    <section>
      {{{contents}}}
    </section>
    <section>
      <h1>List of Posts</h1>
      <ul>
        {{#each collections.posts }}
          <li>
            <a href="{{baseUrl}}/{{this.path}}/" title="{{this.title}}">
              <h2>{{this.title}}</h2>
              <time datetime="{{ dateFormat this.date }}">{{ dateFormat this.date }}</time>
            </a>
          </li>
        {{/each}}
      </ul>
    </section>
  {{> footer}}